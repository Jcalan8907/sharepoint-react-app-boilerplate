{
  "name": "sp-react-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/sp-rest-proxy.js start-dev-server",
    "start-dev-server": "craco start",
    "build": "craco build",
    "test": "node ./bin/sp-rest-proxy.js test-interactive",
    "test-interactive": "craco test",
    "test-coverage": "node ./bin/sp-rest-proxy.js test:coverage",
    "test:coverage": "craco test --coverage",
    "eject": "craco eject",
    "prettier": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "pre-commit-prettier": "pretty-quick --staged",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "run-s pre-commit-prettier test-coverage"
    }
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  },
  "dependencies": {
    "@craco/craco": "^3.1.1",
    "@fortawesome/fontawesome-free": "^5.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@material-ui/core": "^3.6.1",
    "@material-ui/icons": "^3.0.1",
    "@pnp/common": "^1.2.6",
    "@pnp/graph": "^1.2.6",
    "@pnp/logging": "^1.2.6",
    "@pnp/odata": "^1.2.6",
    "@pnp/sp": "^1.2.6",
    "axios": "^0.18.0",
    "clipboard": "^2.0.4",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1",
    "typescript": "3.2.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/clipboard": "^2.0.1",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.9",
    "@types/lodash": "^4.14.118",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.12.10",
    "@types/react": "^16.7.7",
    "@types/react-dom": "^16.0.11",
    "@types/uuid": "^3.4.4",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "husky": "^1.2.0",
    "nock": "^10.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "sp-rest-proxy": "^2.7.7",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0"
  }
}
