{
  "name": "sharepoint-react-app-boilerplate",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=10.13.0"
  },
  "scripts": {
    "start": "node ./bin/sp-rest-proxy.js start-dev-server",
    "start-dev-server": "craco start",
    "build": "craco build",
    "test": "node ./bin/sp-rest-proxy.js test-interactive",
    "test-debug": "node ./bin/sp-rest-proxy.js test-non-interactive",
    "test-interactive": "craco test",
    "test-non-interactive": "CI=true craco --inspect test --no-cache --runInBand",
    "test-coverage": "node ./bin/sp-rest-proxy.js test-ci",
    "test-vscode": "craco test --ci --coverage --reporters=default",
    "test-ci": "craco test --ci --coverage --reporters=default --reporters=jest-junit && node ./bin/inlineCoverageReportCSS.js",
    "analyze": "source-map-explorer build/static/js/main*",
    "analyze-vendor": "source-map-explorer build/static/js/1*",
    "eject": "craco eject",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "pre-commit-prettier": "pretty-quick --staged",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "deploy": "run-s build deploy-build",
    "deploy-build-debug": "node --inspect ./node_modules/.bin/ts-node --project ./bin/tsconfig.json ./bin/uploadFolderToSP.ts \"./build/**\" \"/my-sp-react-app/\"",
    "deploy-build": "ts-node --project ./bin/tsconfig.json ./bin/uploadFolderToSP.ts \"./build/**\" \"/my-sp-react-app/\"",
    "deploy-api": "ts-node --project ./bin/tsconfig.json ./bin/uploadFolderToSP.ts \"./src/api/**\" \"/my-sp-react-app/\"",
    "docker-build": "docker build --rm -t docker-user/sp-react-app:latest .",
    "docker-run": "docker run -p 3000:3000 -p 3001:3001 -d docker-user/sp-react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "homepage": "./",
  "husky": {
    "hooks": {
      "pre-commit": "run-s pre-commit-prettier"
    }
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 200
  },
  "jest": {
    "coverageReporters": [
      "text",
      "html",
      "cobertura"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  },
  "dependencies": {
    "@baristalabs/craco-raw-loader": "^1.0.0",
    "@craco/craco": "^3.4.0",
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@material-ui/core": "^3.9.0",
    "@material-ui/icons": "^3.0.2",
    "@pnp/common": "^1.2.8",
    "@pnp/graph": "^1.2.8",
    "@pnp/logging": "^1.2.8",
    "@pnp/odata": "^1.2.8",
    "@pnp/sp": "^1.2.8",
    "autobind-decorator": "^2.4.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "clipboard": "^2.0.4",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "typescript": "3.2.4",
    "uuid": "^3.3.2",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/clipboard": "^2.0.1",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.13",
    "@types/lodash": "^4.14.120",
    "@types/nock": "^9.3.1",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-test-renderer": "^16.0.3",
    "@types/uuid": "^3.4.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "fs-jetpack": "^2.2.1",
    "husky": "^1.3.1",
    "inline-css": "^2.4.1",
    "jest-junit": "^6.0.1",
    "nock": "^10.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.1",
    "pretty-quick": "^1.10.0",
    "react-test-renderer": "^16.7.0",
    "source-map-explorer": "^1.6.0",
    "sp-pnp-node": "^2.1.3",
    "sp-rest-proxy": "^2.7.7",
    "ts-node": "^8.0.1",
    "tslint": "^5.12.1",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0"
  }
}
