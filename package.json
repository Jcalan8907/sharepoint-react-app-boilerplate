{
  "name": "sp-react-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/sp-rest-proxy.js start-dev-server",
    "start-dev-server": "craco start",
    "build": "craco build",
    "test": "node ./bin/sp-rest-proxy.js test-interactive",
    "test-debug": "node ./bin/sp-rest-proxy.js test-non-interactive",
    "test-interactive": "craco test",
    "test-non-interactive": "CI=true craco --inspect test --no-cache --runInBand",
    "test-coverage": "node ./bin/sp-rest-proxy.js test-ci",
    "test-vscode": "craco test --ci --coverage --reporters=default",
    "test-ci": "craco test --ci --coverage --reporters=default --reporters=jest-junit && node ./bin/inlineCoverageReportCSS.js",
    "eject": "craco eject",
    "prettier": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "pre-commit-prettier": "pretty-quick --staged",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "deploy": "run-s build deploy-build",
    "deploy-build-debug": "node --inspect ./node_modules/.bin/ts-node --project ./bin/tsconfig.json ./bin/uploadFolderToSP.ts './build/**' '/spapp/'",
    "deploy-build": "ts-node --project ./bin/tsconfig.json ./bin/uploadFolderToSP.ts './build/**' '/sp-app/'",
    "deploy-api": "ts-node --project ./bin/tsconfig.json ./bin/uploadFolderToSP.ts './src/api/**' '/sp-app/api/'"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "run-s pre-commit-prettier test-ci"
    }
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 200
  },
  "jest": {
    "coverageReporters": [
      "text",
      "html",
      "cobertura"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  },
  "dependencies": {
    "@baristalabs/craco-raw-loader": "^1.0.0",
    "@craco/craco": "^3.2.3",
    "@fortawesome/fontawesome-free": "^5.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.10",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@material-ui/core": "^3.6.2",
    "@material-ui/icons": "^3.0.1",
    "@pnp/common": "^1.2.7",
    "@pnp/graph": "^1.2.7",
    "@pnp/logging": "^1.2.7",
    "@pnp/odata": "^1.2.7",
    "@pnp/sp": "^1.2.7",
    "axios": "^0.18.0",
    "clipboard": "^2.0.4",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "typescript": "3.2.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/clipboard": "^2.0.1",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.10",
    "@types/lodash": "^4.14.119",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.12.15",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/uuid": "^3.4.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "fs-jetpack": "^2.2.0",
    "husky": "^1.2.1",
    "inline-css": "^2.4.1",
    "jest-junit": "^5.2.0",
    "nock": "^10.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0",
    "sp-pnp-node": "^2.1.3",
    "sp-rest-proxy": "^2.7.7",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0"
  }
}
